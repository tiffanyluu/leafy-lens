    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mime-types": "^3.0.1",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.2",
    "multer": "^1.4.5-lts.2",
